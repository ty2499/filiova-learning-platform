<svg width="32" height="32" viewBox="0 0 32 32" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="eduGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:hsl(345, 65%, 55%);stop-opacity:1" />
      <stop offset="100%" style="stop-color:hsl(350, 70%, 60%);stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background circle -->
  <circle cx="16" cy="16" r="15" fill="url(#eduGradient)" stroke="white" stroke-width="1"/>
  
  <!-- Book/Education icon -->
  <g transform="translate(8, 9)">
    <!-- Book pages -->
    <rect x="2" y="2" width="12" height="12" rx="1" fill="white" opacity="0.9"/>
    <rect x="1" y="1" width="12" height="12" rx="1" fill="white" opacity="0.7"/>
    <rect x="0" y="0" width="12" height="12" rx="1" fill="white"/>
    
    <!-- Book binding -->
    <rect x="0" y="0" width="2" height="12" fill="url(#eduGradient)"/>
    
    <!-- Pages lines -->
    <line x1="3" y1="3" x2="10" y2="3" stroke="hsl(345, 65%, 55%)" stroke-width="0.5" opacity="0.6"/>
    <line x1="3" y1="5" x2="9" y2="5" stroke="hsl(345, 65%, 55%)" stroke-width="0.5" opacity="0.6"/>
    <line x1="3" y1="7" x2="10" y2="7" stroke="hsl(345, 65%, 55%)" stroke-width="0.5" opacity="0.6"/>
    
    <!-- Small "E" for EduFiliova -->
    <text x="6" y="10" font-family="Arial, sans-serif" font-size="4" font-weight="bold" fill="hsl(345, 65%, 55%)" text-anchor="middle">E</text>
  </g>
</svg>